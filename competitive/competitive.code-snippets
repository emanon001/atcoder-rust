    "prefix": "prime_factor",
    "body": [
      "pub fn prime_factor(n: u64) -> std::collections::HashMap<u64, u64> {",
      "    if n < 2 {",
      "        return std::collections::HashMap::new();",
      "    }",
      "    let mut res = std::collections::HashMap::new();",
      "    let mut n = n;",
      "    let mut i = 2;",
      "    while i * i <= n {",
      "        while n % i == 0 {",
      "            let c = res.entry(i).or_insert(0);",
      "            *c += 1;",
      "            n /= i;",
      "        }",
      "        i += 1;",
      "    }",
      "    if n != 1 {",
      "        res.insert(n, 1);",
      "    }",
      "    res",
      "}"
    ]
  },
